/*工作协同、计划、日志 列表页*/
.tabList{
  width: 100%;
  margin-top: 5px;
  border-bottom: 1px solid #dcdfe6;
  overflow: hidden;
}
.tabList ul{
  overflow: hidden;
}
.tabList ul li{
  float: left;
  width: auto;
  padding: 0 20px;
  margin: 0 10px;
  line-height: 32px;
  cursor: pointer;
}
.tabList ul li:first-child{
  margin-left: 0;
}
.tabList ul li.active{
  color: #498ce9;
  border-bottom: 2px solid #498ce9;
}
.tableTile i{
  width: 10px;
  height: 10px;
  -webkit-border-radius: 50%;
  -moz-border-radius: 50%;
  border-radius: 50%;
  display: inline-block;
}
.tableTile i.read{
  background: #dddddd;
}
.tableTile i.unread{
  background: #fe8d24;
}
.tableTile em{
  display: inline-block;
  width: 60px;
  height: 23px;
  font-style: normal;
  text-align: center;
  font-size: 12px;
}
.tableTile em.read{
  background: #e8f2ff;
  color: #a6bfee;
}
.tableTile em.unread{
  background: #c9e0ff;
  color: #1465d6;
}
.tableTile span.read{
  color: #dddddd;
}
.tableTile span.unread{
  color: #333333;
}

