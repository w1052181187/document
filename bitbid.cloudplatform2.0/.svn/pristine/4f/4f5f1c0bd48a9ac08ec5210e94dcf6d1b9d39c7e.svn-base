import request from '@/utils/axios/components-axios'

export const archivesInfo = {
  /** 查询 */
  getArchivesInfoList (params) {
    return request({
      url: '/archives-infos',
      method: 'get',
      params: params
    })
  },
  /** 查询详情 */
  detailArchivesInfo (objectId) {
    return request({
      url: '/archives-infos/' + objectId,
      method: 'get'
    })
  },
  /** 保存 */
  saveArchivesInfo (data) {
    return request({
      url: '/archives-infos',
      method: 'post',
      data: data
    })
  },
  /** 修改 */
  updateArchivesInfo (data) {
    return request({
      url: '/archives-infos',
      method: 'put',
      data: data
    })
  },
  /** 删除文件目录 */
  deleteArchivesInfo (objectId) {
    return request({
      url: '/archives-catalogs/' + objectId,
      method: 'delete'
    })
  },
  /** 项目  归档 */
  archivesFile (data) {
    return request({
      url: '/archives-infos/archivesFile',
      method: 'put',
      data: data
    })
  }
}
