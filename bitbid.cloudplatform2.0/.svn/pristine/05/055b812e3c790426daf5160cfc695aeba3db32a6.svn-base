  border-color: #62c08e;
}
button.cancal{
  background-color: #e4e4e4;
  border-color: #e4e4e4;
  color: #666666;
}
button.cancal:hover{
  background-color: #f3f3f3;
  border-color: #f3f3f3;
  color: #666666;
}

/*高级搜索*/
.advancedsearch_box{
  width: 100%;
}
.advancedsearch_box .el-checkbox-group{
  display: inline-block;
}
.advancedsearch_box .el-input__inner{
  line-height: 32px!important;
}
.advancedsearch_box span.more{
