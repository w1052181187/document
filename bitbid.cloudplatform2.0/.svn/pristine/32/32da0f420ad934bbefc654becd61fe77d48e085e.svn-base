import request from '@/utils/axios/components-axios'

export const templateInfo = {
  /**
   * 查询模板信息接口
   */
  getList (query) {
    return request({
      url: '/templateInfo-infos',
      method: 'get',
      params: query
    })
  },
  /**
   * 查询单条模板信息接口
   */
  getOne (code) {
    return request({
      url: '/templateInfo-infos/' + code,
      method: 'get'
    })
  },
  /**
   * 新增模板信息接口
   * @param {*} data [主体数据]
   */
  save (data) {
    return request({
      url: '/templateInfo-infos',
      method: 'post',
      data: data
    })
  },
  /**
   * 删除模板信息接口
   * @param {[long]} id [主键]
   */
  delete (id) {
    return request({
      url: '/templateInfo-infos/' + id,
      method: 'delete'
    })
  },
  /**
   * 修改发布状态
   */
  updateStatus (data) {
    return request({
      url: '/templateInfo-infos/updateStatus',
      method: 'post',
      data: data
    })
  }
}
