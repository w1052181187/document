              <el-row class="title">{{scope.row.name}}</el-row>
              <el-row class="line"></el-row>
              <el-row :gutter="12">
                <el-col :span="4" class="room_status">
                  <span>·</span>
                  <span v-if="scope.row.appointmentRecordMap.t195.businessType === 1">开标</span>
                  <span v-if="scope.row.appointmentRecordMap.t195.businessType === 2">评标</span>
                  <span v-if="scope.row.appointmentRecordMap.t195.businessType === 3">开标/评标</span>
                </el-col>
                <el-col :span="12" class="company" v-if="scope.row.appointmentRecordMap.t195 && scope.row.appointmentRecordMap.t195.status === 1">{{scope.row.appointmentRecordMap.t195.tenderProjectName}}</el-col>
                <el-col :span="12" class="company" v-else>该时段已被禁用</el-col>
                <el-col :span="8" class="time">{{scope.row.appointmentRecordMap.t195.usedDate}}</el-col>
              </el-row>
            </div>
            <span v-else>暂无预约</span>
            <div slot="reference">
              <div class="room my_select_room" v-if="scope.row.appointmentRecordMap.t195 && scope.row.appointmentRecordMap.t195.status === 1 && scope.row.appointmentRecordMap.mine195"></div>
              <div class="room select_room" v-if="scope.row.appointmentRecordMap.t195 && scope.row.appointmentRecordMap.t195.status === 1 && !scope.row.appointmentRecordMap.mine195"></div>
              <div class="room ban_room" v-if="scope.row.appointmentRecordMap.t195 && scope.row.appointmentRecordMap.t195.status === 2"></div>
              <div class="room" v-if="!scope.row.appointmentRecordMap.t195"></div>
            </div>
          </el-popover>
        </template>
      </el-table-column>
      <el-table-column
        label="操作"
        align="center"
        width="50"
        fixed="right">
        <template slot-scope="scope">
          <el-button type="text" size="small" @click="handleOrder(scope)">预约</el-button>
        </template>
      </el-table-column>
    </el-table>
    <el-row class="hint" type="flex">
      <el-col :span="1" class="hint_txt">提示：</el-col>
      <el-col :span="2">
        <div class="order">
          <span></span>他人预约
        </div>
      </el-col>
      <el-col :span="2">
        <div class="order my">
          <span></span>我的预约
        </div>
      </el-col>
      <el-col :span="2">
        <div class="order no">
          <span></span>未预约
        </div>
      </el-col>
      <el-col :span="2">
        <div class="order ban">
          <span></span>禁用
        </div>
      </el-col>
    </el-row>
    <!--分页-->
    <el-pagination
      background
      layout="prev, pager, next"
      :total="total"
      :page-size='pageSize'
      :current-page.sync="currentPage"
      @current-change="handleCurrentChange">
    </el-pagination>
    <!--分页-->
  </div>
</template>

<script>
import {bidOpeningRoom} from '@/api/resource'
export default {
  name: '',
  data () {
    return {
      searchForm: {
        time: null,
        checked: 0
      },
      tableData: [
      ],
      // 当前页
      currentPage: 1,
      pageNo: 0,
      total: 0, // 总条数
      pageSize: 5 // 每页展示条数
    }
  },
  methods: {
    // 分页
    handleCurrentChange (nowNum) {
      this.currentPage = nowNum
      this.pageNo = (nowNum - 1) * this.pageSize
      this.getTableData()
    },
    // 搜索时间改变
    handleDateChange () {
      this.getTableData()
    },
    // 选择开标或者评标
    handleTypeChange (value) {
      this.pageNo = 0
      this.currentPage = 1
      this.getTableData()
    },
    handleOrder (scope) {
      this.$emit('addOrder', scope)
    },
    getTableData () {
      let date = new Date()
      // 初始化当前时间
      if (!this.searchForm.time) {
        this.searchForm.time = date.getFullYear() + '-' + (date.getMonth() + 1) + '-' + date.getDate() + ' 00:00:00'
      }
      let query = {
        pageNo: this.pageNo,
        pageSize: this.pageSize,
        queryDate: this.searchForm.time + ' 00:00:00'
      }
      if (this.searchForm.checked === 1) {
        this.$set(query, 'businessTypes', '1,3')
      } else if (this.searchForm.checked === 2) {
        this.$set(query, 'businessTypes', '2,3')
      } else if (this.searchForm.checked === 3) {
        this.$set(query, 'businessTypes', '3')
      }
      bidOpeningRoom.getBidOpeningRoomAndRecordList(query).then(res => {
        if (res.data.resCode === '0000') {
          this.tableData = res.data.bidOpeningRoomList.list
          this.total = res.data.bidOpeningRoomList.total
        } else {
          return false
        }
      })
    }
  },
  mounted () {
    this.getTableData()
  }
}
</script>

