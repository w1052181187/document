<script>
import {bidOpeningRoom} from '@/api/resource'
export default {
  name: '',
  data () {
    return {
      searchForm: {
        time: null,
        checkList: []
      },
      tableData: [
      ],
      // 当前页
      currentPage: 1,
      pageNo: 0,
      total: 0, // 总条数
      pageSize: 10 // 每页展示条数
    }
  },
  methods: {
    // 分页
    handleCurrentChange (nowNum) {
      this.currentPage = nowNum
      this.pageNo = (nowNum - 1) * this.pageSize
      this.getList()
    },
    // 搜索时间改变
    handleDateChange () {
      this.getTableData()
    },
    // 选择开标或者评标
    handleTypeChange (value) {
      this.getTableData()
    },
    handleOrder (scope) {
      this.$emit('addOrder', scope)
    },
    getTableData () {
      let date = new Date()
      // 初始化当前时间
      this.searchForm.time = date.getFullYear() + '-' + (date.getMonth() + 1) + '-' + date.getDate() + ' 00:00:00'
      bidOpeningRoom.getBidOpeningRoomAndRecordList({
        pageNo: this.pageNo,
        pageSize: this.pageSize,
        businessTypes: this.searchForm.checkList.join(),
        queryDate: this.searchForm.time
      }).then(res => {
        this.tableData = res.data.bidOpeningRoomList.list
        this.total = res.data.bidOpeningRoomList.total
      })
    }
  },
  mounted () {
    this.getTableData()
  }
}
</script>

