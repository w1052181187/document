/*各列表 可点击 添加手形*/
.pointer{
  cursor: pointer;
}

/** 项目标段列表 **/
.section_listbox{
  width: 100%;
  overflow: hidden;
}
.section_listbox .btnMore i {
  float: right;
  font-style: normal;
  font-size: 12px;
  padding-left: 18px;
  margin-top: 5px;
  cursor: pointer;
}
.section_listbox i.open {
  background: url("../../../static/images/ico_isopen.png") left center no-repeat;
}
.section_listbox i.close {
  background: url("../../../static/images/ico_isclose.png") left center no-repeat;
}
.section_list{
  height: 48px;
  overflow: hidden;
}
.section_list ul{
  height: auto;
  overflow: hidden;
}
.section_list ul li{
  height: 44px;
  padding: 0 12px;
  border: 1px solid #d9d9d9;
  max-width: 210px;
  min-width: 100px;
  text-align: center;
  -webkit-box-sizing: border-box;
  -moz-box-sizing: border-box;
  box-sizing: border-box;
  float: left;
  margin: 4px;
  font-size: 12px;
  line-height: 20px;
  cursor: pointer;
}
.section_list ul li.cur{
  background: #40a3f1;
  border: 1px solid #498ce9;
  color: #ffffff;
}
.section_list ul li.cur p{
  color: #ffffff;
}
.section_list ul li.cur p i{
  color: #ffffff;
}
.section_list ul li.cur span{
  color: #ffffff;
}
.section_list ul li p{
  color: #666666;
  max-width: 100%;
  overflow: hidden;
  -ms-text-overflow: ellipsis;
  text-overflow: ellipsis;
  white-space: nowrap;
  font-weight: bold;
}
.section_list ul li p i{
  color: #fe903f;
  font-style: normal;
}
.section_list ul li span{
  color: #40a3f1;
}
