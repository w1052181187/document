                </el-date-picker>
              </el-col>
            </el-row>
            <el-row>
              <el-col :span="9" class="region">
                <el-form-item label="所在地区：">
                  <el-select v-model="searchForm.provinceId"  @change="getCityNode()" placeholder="请选择">
                    <el-option
                      v-for="item in regionOptions"
