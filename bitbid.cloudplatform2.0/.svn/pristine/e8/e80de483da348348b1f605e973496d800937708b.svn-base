<template>
  <div class="cloudcontent" id="cloud_index">
    <el-row :gutter="20">
      <el-col :span="leftnum" class="leftcontent">
        <div class="message">
          <div class="fl">
            <h3 class="mestitle">待开标 <img src="../../static/images/newindex/ico_tit1.png"/></h3>
            <ul class="meslist">
              <li>
