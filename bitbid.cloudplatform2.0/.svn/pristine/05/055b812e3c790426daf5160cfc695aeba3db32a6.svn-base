/* table 下 input margin*/
.el-table__row .el-form-item__content{
  margin-left: 0px!important;
}
.file-type-name {
  color: #333;
  font-weight: 800;
  margin-bottom: 12px;
  font-size: 14px;
}
.file-type-add{
  cursor: pointer;
  border: #dcdfe6 1px dashed;
  height: 54px;
  line-height: 54px;
  text-align: center;
  margin-top: 8px;
  color: #999;
  font-size: 14px;
}
.file-type-enclosure {
  margin-bottom: 12px;
  margin-top: 12px;
}

/* 开评标室管理-检测是否可预约 */
.check_order{
  color: #108cee;
  font-size: 12px;
  text-decoration: underline;
  margin-left: 12px;
  cursor: pointer;
  display: inline-block;
}
/* element-ui表格show-overflow-tooltip="true"，鼠标移上去显示的宽度设置 */
.el-tooltip__popper {
  max-width: 800px;
  word-break: break-all!important;
}

/*修改竞争对手 表格 表头2行显示高度*/
.short_th th{
  padding: 6px 0;
}

/*二期迭代 修改所有详情页 高度间距*/
.cloudcontent .viewdetails .el-form-item{
  margin-bottom: 3px;
}
/* 修改详情或添加页面  相关标段附件等表格表头高度*/
.el-table th, .el-table tr{
  line-height: 23px;
}

/*详情页table*/
.detailtable{
  width: 100%;
  border-collapse: collapse;
  font-size: 14px;
  table-layout: fixed;
  margin-bottom: 15px;
}
.detailtable>tr{
  overflow: hidden;
}
.detailtable>tr>td{
  border: 1px solid #DDDDDD;
  padding: 5px 10px;
  box-sizing: border-box;
}
.detailtable>tr>td>span{
  display: inline-block;
  width: 100%;
  overflow: hidden;
  text-overflow:ellipsis;
  white-space: nowrap;
  text-align: left;
}
/** 档案管理*/
.bid-code {
  font-weight: 800;
}
.transfer-info {
  border: #dcdfe6 1px solid;
  /*height: 60px;*/
  /*line-height: 60px;*/
  padding-top: 12px;
  box-sizing: border-box;
  /*font-size: 14px;*/
  /*margin-bottom: 24px;*/
}
.file-item {
  width: 100%;
  background-color: #eeeeee;
  font-size: 14px;
  padding: 20px 32px;
  /*padding-bottom: 20px;*/
  /*padding-left: 32px;*/
  box-sizing: border-box;
}
.file-item-gap {
  width: 100%;
  background-color: #eeeeee;
  font-size: 14px;
  padding: 20px 32px;
  /*padding-top: 20px;*/
  /*padding-bottom: 20px;*/
  /*padding-left: 32px;*/
  box-sizing: border-box;
}
.file-name {
  color: #111;
}
.file-action {
  float: right;
  cursor: pointer;
}
.file-action-active {
  float: right;
  cursor: pointer;
  color: #498ce9;
}
.file-action-del {
  width: 14px;
  height: 16px;
  vertical-align: middle;
  margin-left: 10px;
  cursor: pointer;
}
.file-action-txt {
  vertical-align: middle;
  margin-right: 10px;
}
.pack-down {
  width: 12px;
  height: 12px;
  vertical-align: middle;
}
.pack-reduce {
  width: 12px;
  height: 2px;
  vertical-align: middle;
}
.file-box .file-item-border{
  border: #dcdfe6 1px solid;
  margin-top: 8px;
}
.add-file {
  margin-top: 24px!important;;
  background: #108cee;
  border-color: #108cee;
  color: white;
}
.module {
  margin-top: 12px;
  overflow: hidden;
}
.module .left {
  width: 12%;
  float: left;
  border: #dee1e5 1px solid;
}
.module .middle {
  width: 2%;
  height: 100px;
  float: left;
}
.module .right {
  width: 85%;
  float: left;
}
.module .title {
  margin: 16px 16px;
  height: 18px;
  line-height: 18px;
  display: inline-block;
  /*text-align: center;*/
  font-weight: 800;
}
.module .title::before {
  display: inline-block;
  content: '';
  width: 3px;
  height: 18px;
  margin-right: 6px;
  background-color: #498ce9;
  text-align: center;
  vertical-align: middle;
}
.module .active-menu {
  color: #498ce9;
}
.module .left-menu li {
  padding: 16px 16px;
  box-sizing: border-box;
  vertical-align: middle;
  border-top: #eeeeee 1px solid;
  cursor: pointer;
}
.module .left-menu li img {
  width: 15px;
  height: 15px;
  vertical-align: middle;
  margin-right: 6px;
}
.module .left-menu li span {
  display: inline-block;
  width: 15px;
  height: 15px;
  vertical-align: middle;
  margin-right: 6px;
}
.module .right .line {
  width: 100%;
  height: 1px;
  background-color: #eeeeee;
  margin-bottom: 10px;
}
.module .right .content {
  margin-left: 16px;
}
.module .right .explain {
  margin-bottom: 10px;
  color: #333;
  font-weight: 800;
}
.module .right .explain-box {
  margin-bottom: 30px;
}
.basic-info-title {
  font-weight: 800;
  margin: 12px 0;
  font-size: 17px;
}
.basic-info-title::before {
  display: inline-block;
  content: '';
  width: 3px;
  height: 24px;
  margin-right: 6px;
  background-color: #498ce9;
  vertical-align: middle;
}
.file-pack-check-all {
  height: 30px;
  line-height: 30px;
  margin-bottom: 12px;
}
.file-pack-check-all .pack-check {
  float: right;
  margin-right: 24px;
}
.file-pack-check-all .pack-btn {
  float: right;
  color: #fff;
  background: #108cee;
  border-color: #108cee;
  height: 30px;
  padding-top: 0;
  padding-bottom: 0;
}
.file-pack-check-all .pack-btn span {
  margin-left: 6px;
}
.tender-project-name img{
  width: 12px;
  height: 12px;
  margin-right: 10px;
  vertical-align: middle;
}
.project-info-line {
  height: 10px;
  width: 100%;
  background-color: #eaeef2;
}
.transferStatus-accept {
  color: #3bb072;
}
.detail-title{
  padding: 12px 0;
  box-sizing: border-box;
  color: #333;
  font-weight: 800;
  font-size: 17px;
}
.has-file-img {
  width: 15px;
  height: 15px;
  vertical-align: middle;
  margin-left: 6px;
  display: inline-block;
}
ul.step{
  list-style: none;
  width: 640px;
  height: 30px;
  /*cursor: pointer;*/
  overflow: hidden;
  margin: 20px auto;
  text-align: center;
  /*margin-top: 20px;*/
}
ul.step li{
  float: left;
  height: 30px;
  margin-right: 150px;
  position: relative;
  display: inline-block;
}
ul.step li i{
  display: inline-block;
  width: 30px;
  height: 30px;
  line-height: 30px;
  -webkit-border-radius: 50%;
  -moz-border-radius: 50%;
  border-radius: 50%;
  background: #bbbbbb;
  color: #ffffff;
  font-style: normal;
  float: left;
}
ul.step li img {
  width: 17px;
  height: 11px;
}
ul.step li span{
  display: inline-block;
  line-height: 30px;
  margin-left: 10px;
  font-size: 14px;
  color: #999;
  float: left;
}
ul.step li:last-child{
  margin-right: 0px;
}
ul.step li em{
  position: absolute;
  width: 104px;
  height: 2px;
  display: inline-block;
  /*left: 142px;*/
  top: 15px;
  background: #eeeeee;
  margin-left: 24px;
}
ul.step li.prev em{
  background: #498ce9;
}
ul.step li.active i{
  background: #498ce9;
}
ul.step li.active span{
  color: #333333;
}
ul.step li.prev i{
  background: #498ce9;
}
ul.step li.prev span{
  color: #333;
}
